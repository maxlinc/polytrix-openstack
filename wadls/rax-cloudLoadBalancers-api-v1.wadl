                        <resource id="metadata"
                path="v1.0/{account}/loadbalancers/{loadBalancerId}/metadata">
         <param name="account" style="template" type="xsd:string">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>The ID for the tenant or account in a multi-tenancy
            cloud.</para>
            </wadl:doc>
         </param>
         <param style="template"
                type="xsd:string"
                required="true"
                name="loadBalancerId">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>The ID for the load balancer.</para>
            </wadl:doc>
         </param>
         <method name="POST" rax:id="addLoadBalancerMetadata">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Add load balancer metadata">
               <para role="shortdesc">Adds a metadata item to the load
                balancer.</para>
               <para>When a metadata item is added, it is assigned a
                unique identifier that can be used for mutating
                operations such as changing the value attribute or
                removing it.</para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code>
                        <programlisting language="javascript">{
  "meta": {
    "value":"blue"
  }
}</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
               </representation>
               <representation mediaType="application/xml" xml:lang="EN" element="lbapi:meta">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code>
                        <programlisting language="xml">&lt;meta xmlns="http://docs.openstack.org/loadbalancers/api/v1.0"&gt;blue&lt;/meta&gt;</programlisting>
                     </xsdxt:code>
                  </wadl:doc> 
                  <param name="key" style="plain" required="true" type="xsd:string">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>Key that is used to identify the metadata. Must be 256
            characters or fewer. All UTF-8 characters are valid.</para>
                     </wadl:doc>
                  </param> 
                  <param name="value" style="plain" required="true" type="xsd:string">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>Value for the metadata item. Must be 256 characters or
            less. All UTF-8 characters are valid.</para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
            </representation>
            </response> 
            <response status="400 500">
               <representation mediaType="application/xml" element="lbapi:loadbalancerFault"/>
               <representation mediaType="application/json"/>
            </response>
            <response status="503">
               <representation mediaType="application/xml" element="lbapi:serviceUnavailable"/>
               <representation mediaType="application/json"/>
            </response>
            <response status="400">
               <representation mediaType="application/xml" element="lbapi:badRequest"/>
               <representation mediaType="application/json"/>
            </response>
            <response status="401">
               <representation mediaType="application/xml" element="lbapi:unauthorized"/>
               <representation mediaType="application/json"/>
            </response>
        
        
            <response status="413">
               <representation mediaType="application/xml" element="lbapi:overLimit"/>
               <representation mediaType="application/json"/>
            </response>
            
            <response status="404">
               <representation mediaType="application/xml" element="lbapi:itemNotFound"/>
               <representation mediaType="application/json"/>
            </response>
         </method>
         <method name="GET" rax:id="showLoadBalancerMetadata">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Show load balancer metadata">
               <para role="shortdesc">Shows all metadata associated with
                the specified load balancer.</para>
            </wadl:doc>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code>
                        <programlisting language="javascript">{
  "metadata": [
    {
      "id":"1",
      "key":"color",
      "value":"red"
    },
    {
      "id":"2",
      "key":"label",
      "value":"web-load-balancer"
    }
  ]
}</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
               </representation>
               <representation mediaType="application/xml" xml:lang="EN" element="lbapi:metadata">
