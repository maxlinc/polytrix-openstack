         <method name="POST" rax:id="createNetwork">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Create Network">
               <para role="shortdesc">Creates a network.</para>
               <para>This operation creates a network. The tenant ID that you specify in the URI is the tenant who creates the
                network.</para>
               <note>
                  <para>Only one <literal>IPv4</literal> and one <literal>IPv6</literal> subnet can be specified per
                Network.</para>
               </note>
            </wadl:doc>
            <request>
               <representation mediaType="application/json" element="csapi:network">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "network": 
    {
        "admin_state_up": true,
        "id": "4d4e772a-98e7-4409-8a3c-4fed4324da26",
        "name": "testnet-3",
        "shared": false,
        "status": "ACTIVE",
        "subnets": [],
        "tenant_id": "123456"
    }
}</programlisting>
                     </xsdxt:code>
                  </wadl:doc> 
                  <param required="true"
                         name="network"
                         style="plain"
                         type="object"
                         path="$.parameters.network">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The container for the network details.</para>
                     </wadl:doc>
                  </param>
                  <param required="false"
                         name="admin_state_up"
                         style="plain"
                         type="xsd:bool"
                         path="$.parameters.network.admin_state_up">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The administrative state of the network. The default value is <literal>true</literal> 
              and cannot be changed by the user.</para>
                     </wadl:doc>
                  </param>
                  <param required="false"
                         name="name"
                         style="plain"
                         type="xsd:string"
                         path="$.parameters.network.name">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The network name.  Currently, network names should be 40 characters or fewer.</para>
                     </wadl:doc>
                  </param>
                  <param required="false"
                         name="shared"
                         style="plain"
                         type="xsd:bool"
                         path="$.parameters.network.shared">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>Admin only. Indicates whether this network is shared across all tenants. The default value is <literal>false</literal> 
              and cannot be changed by the user.</para>
                     </wadl:doc>
                  </param>
                  <param required="false"
                         name="tenant_id"
                         style="plain"
                         type="csapi:uuid"
                         path="$.parameters.network.tenant_id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The ID of the tenant who owns the network.</para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="201">
               <representation mediaType="application/json" element="csapi:network">
